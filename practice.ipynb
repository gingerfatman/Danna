    " s = 'Today was a lovely day. I took a trip to the beach.'\n",
    " \n",
    " for letter in s.lower():\n",
    "     if letter in 'aeiou':\n",
    "         cnt += 1\n",
    "\n",
    "f'This sentence has {cnt} vowels'\n",
    "\n"
